   "source": [
    "def T_aray(T_hot, T_cold, N):\n",
    "    T = [T_hot * (T_cold / T_hot) ** (i / (N - 1)) for i in range(N)]\n",
    "    return T\n",
    "\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "plt.scatter(T_aray(100, 1.0, 100), [0 for i in range(100)])"
   ]
